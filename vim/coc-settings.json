{
  "cSpell.fixSpellingWithRenameProvider": true,
  "cSpell.import": ["~/work/.cspell.json"],
  "cSpell.maxDuplicateProblems": 1,
  "cSpell.userWords": [],
  "coc.preferences.formatOnSave": true,
  "coc.preferences.watchmanPath": "/usr/local/bin/watchman",
  "deno.inlayHints.enumMemberValues.enabled": true,
  "colors.enable": true,
  "deno.inlayHints.parameterNames.enabled": "literals",
  "deno.inlayHints.parameterTypes.enabled": true,
  "deno.inlayHints.propertyDeclarationTypes.enabled": true,
  "deno.inlayHints.variableTypes.enabled": true,
  "deno.path": "/usr/bin/deno",
  "diagnostic.errorSign": "",
  "diagnostic.hintSign": "﯎",
  "diagnostic.infoSign": "",
  "diagnostic.virtualText": true,
  "eslint.autoFixOnSave": true,
  "eslint.run": "onSave",
  "git.addGBlameToVirtualText": true,
  "git.enableGutters": true,
  "javascript.inlayHints.enumMemberValues.enabled": true,
  "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
  "javascript.inlayHints.parameterNames.enabled": "literals",
  "javascript.inlayHints.parameterTypes.enabled": true,
  "javascript.inlayHints.propertyDeclarationTypes.enabled": true,
  "javascript.inlayHints.variableTypes.enabled": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "npm.binPath": "/usr/local/bin/npm",
  "toml.schema.associations": {
    "^(.*(/|\\\\)\\.?starship.toml)$": "https://starship.rs/config-schema.json",
    "^(.*(/|\\\\)\\.?taplo\\.toml|\\.?taplo\\.toml)$": "taplo://taplo.toml"
  },
  "toml.schema.links": true,
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.parameterNames.enabled": "literals",
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "yaml.format.enable": true
}
